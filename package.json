{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "rm -f public/css/*.br && rm -f public/css/*.gz && rm -f public/js/*.br && rm -f public/js/*.gz && mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "rm -f public/css/*.br && rm -f public/css/*.gz && rm -f public/js/*.br && rm -f public/js/*.gz && mix watch --hot",
    "prod": "npm run production && npm run compress",
    "production": "mix --production",
    "compress": "bread-compressor -s public/css/*.css public/js/*.js"
  },
  "devDependencies": {
    "axios": "^0.18",
    "bread-compressor-cli": "^1.1.0",
    "bootstrap": "~4.3.1",
    "bootstrap-vue": "^2.0.2",
    "cross-env": "^5.2.0",
    "jquery": "^3.3.1",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.5",
    "popper.js": "^1.12",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.54.8",
    "sass-loader": "^12.6.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "foundation-emails": "^2.2.1",
    "lightbox2": "^2.10.0",
    "moment": "^2.22.2",
    "vee-validate": "^2.1.4",
    "vue-flash": "^2.1.3",
    "vue-ls": "^2.4.1",
    "vue-masonry": "^0.11.1",
    "vue-router": "^3.0.2",
    "vue-sweetalert2": "^1.2.4",
    "vue2-editor": "^2.4.3"
  }
}
